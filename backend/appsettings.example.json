{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Hangfire": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=fotoai;Username=fotoai_user;Password=dev_password_change_in_production",
    "Redis": "localhost:6379"
  },
  "JWT": {
    "Secret": "change-this-to-a-secure-random-string-in-production",
    "Issuer": "FotoAI",
    "Audience": "FotoAI-API",
    "ExpirationHours": 24
  },
  "Storage": {
    "Type": "Local",
    "LocalPath": "./uploads",
    "ProcessedPath": "./processed",
    "MaxFileSizeMB": 100
  },
  "AI": {
    "AnalysisServiceUrl": "http://localhost:8001",
    "RawPipelineUrl": "http://localhost:8002",
    "JpgPipelineUrl": "http://localhost:8003",
    "SeoServiceUrl": "http://localhost:8004",
    "TimeoutSeconds": 300
  },
  "Processing": {
    "MaxConcurrentJobs": 3,
    "MaxPhotosPerBatch": 150,
    "RetryAttempts": 3
  },
  "Cors": {
    "AllowedOrigins": ["http://localhost:3000", "https://app.fotoai.com"]
  }
}
